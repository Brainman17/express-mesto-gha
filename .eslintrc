{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}]
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "react"
  ]
}

